<Group labelreference="L558" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">ratconvert := proc(c,m)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">  local u, v, q, r;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">  u := [1,0,m];  v := [0,1,c];  print(v);</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">  while evalf(sqrt(m/2)) &lt;= v[3] do</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">    q := iquo(u[3],v[3]);</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">    r := u - q*v;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">    u := v;  v := r;  print(v);</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">  end do;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">  if abs(v[2]) &gt;= evalf(sqrt(m/2)) then</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">    return [];</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">  else</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">    return [v[3],v[2]];</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">  end if;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">end proc;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
</Input>
<Output>
<Text-field style="2D Output" layout="Maple Output"><Equation executable="false" style="2D Output" display=""></Equation></Text-field>
</Output>
