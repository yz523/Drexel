  i := ind[1];  j := ind[2]; a := A[i,i];  b := A[j,i]; </Text-field>
</Input>
<Input>
<Text-field prompt="&gt; " style="Maple Input" alignment="left" firstindent="0" spacebelow="0" leftmargin="0" linespacing="0.0" initial="0" linebreak="space" rightmargin="0" bulletsuffix="" spaceabove="0" bullet="none" pagebreak-before="false">  if (a = b) then g := a; x := 1; y := 0; else g := igcdex(a,b,'x','y');  end if;</Text-field>
